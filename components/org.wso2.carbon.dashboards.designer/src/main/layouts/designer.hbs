<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-COMPATIBLE" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {{placeholder "css"}}
    {{defineZone "header"}}
    {{#placeholder "favicon"}}
        <link rel="shortcut icon" href="{{public "images/default-favicon.png"}}" type="image/png"/>
    {{/placeholder}}
    <title>{{#placeholder "title"}}{{/placeholder}}</title>

</head>
<body>
{{defineZone "task-bar"}}
{{defineZone "left-nav-bar"}}
<div class="sidebar-wrapper" id="left-sidebar" data-side="left" data-width="250"
     data-offset-left="52" data-top="inherit" data-offset-bottom="40" 
     data-container=".page-content-wrapper" data-container-divide="true">
    {{defineZone "sidebar-nav-layouts"}}
    {{defineZone "sidebar-nav-widgets"}}
</div>
<div class="sidebar-wrapper" id="right-sidebar" data-side="right" data-width="250"
     data-offset-left="52" data-top="inherit" data-offset-bottom="40" 
     data-container=".page-content-wrapper" data-container-divide="true">
    {{defineZone "sidebar-left-nav-config"}}
</div>
{{defineZone "content"}}

{{defineZone "footer"}}
<footer class="hidden">
    {{placeholder "js"}}
</footer>
</body>
</html>