                    <button class="btn btn-default ues-custom-action" data-action="{{action}}" type="button" title="{{toolTip}}">
                        {{#if iconTypeCSS}}
                        <i class="icon fw {{iconClass}}"></i>
                        {{/if}}
                        {{#if iconTypeImage}}
                        <img src="{{resolveURI this.image}}"/>
                        {{/if}}
                    </button>
                </li>
                {{/each}}

                {{#if default.configurations}}
