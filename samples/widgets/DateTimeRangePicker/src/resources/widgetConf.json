{
    "id": "DateRangePicker",
    "name": "Date Time Range",
    "thumbnailURL": "",
    "configs": {
        "pubsub": {
            "types": [
                "publisher"
            ],
            "publisherWidgetOutputs":[
                "granularity",
                "from",
                "to"
            ]
        },
        "options": [
            {
                "id": "defaultValue",
                "title": "Default Range",
                "type": {
                    "name": "ENUM",
                    "possibleValues": [
                        "1 Min",
                        "15 Min",
                        "1 Hour",
                        "1 Day",
                        "7 Days",
                        "1 Month",
                        "3 Months",
                        "6 Months",
                        "1 Year"
                    ]
                },
                "defaultValue": "3 Months"
            },
            {
                "id": "availableGranularities",
                "title": "Available Granularities",
                "type": {
                    "name": "ENUM",
                    "possibleValues": [
                        "From Second",
                        "From Minute",
                        "From Hour",
                        "From Day",
                        "From Month",
                        "From Year"
                    ]
                },
                "defaultValue": "From Second"
            },
            {
                "id": "autoSyncInterval",
                "title": "Auto-Sync Interval (Seconds)",
                "type": {
                    "name": "TEXT"
                },
                "defaultValue": "10"
            }
        ]
    }
}
